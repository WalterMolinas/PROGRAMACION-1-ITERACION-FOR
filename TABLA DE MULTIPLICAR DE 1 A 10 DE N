#include <stdio.h>
//Pide al usuario un n√∫mero y muestra su tabla de multiplicar del 1 al 10.
int main() {
	int numero,i,result;
	printf("Ingrese la tabla del numero que desea mostrar\n");
	scanf("%d", &numero);
	for (i=1;i<=10;i++){
		result=numero*i;
		printf("%d x %d = %d \n",numero,i,result);
	}
	return 0;
}
